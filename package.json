{
  "name": "oscar-extension",
  "version": "1.3.0",
  "description": "OSCAR - Website Compliance Scanner. Scan websites for privacy policies, cookie banners, terms of service, and 26 other compliance elements.",
  "main": "background/service-worker.js",
  "scripts": {
    "test": "node tests/run-all.js",
    "test:manifest": "node tests/manifest.test.js",
    "test:patterns": "node tests/patterns.test.js",
    "test:files": "node tests/files.test.js",
    "build": "node scripts/build.js",
    "lint": "echo 'No linter configured'"
  },
  "keywords": [
    "chrome-extension",
    "compliance",
    "privacy",
    "gdpr",
    "ccpa",
    "cookie-consent",
    "terms-of-service",
    "accessibility",
    "legal",
    "scanner"
  ],
  "author": "Compliance Pals",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wbuf81/oscar-extension.git"
  },
  "bugs": {
    "url": "https://github.com/wbuf81/oscar-extension/issues"
  },
  "homepage": "https://github.com/wbuf81/oscar-extension#readme",
  "engines": {
    "node": ">=14.0.0"
  }
}
